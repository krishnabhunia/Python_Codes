    "lst = [0,1]\n",
    "def fib(n):\n",
    "    if n >= len(lst):\n",
    "        res = fib(n-2) + fib(n-1)\n",
    "        lst.append(res)\n",
    "        return lst[n]\n",
    "    else:\n",
    "        return lst[n]\n",
    "        \n",
    "fib(10)\n",
    "print(lst[-1])"
